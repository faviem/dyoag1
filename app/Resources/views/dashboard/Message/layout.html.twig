{% extends 'layout.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
    <link href='//fonts.googleapis.com/css?family=Roboto:700,500,300,100italic,100,400' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="{{ asset('assets/css/dashboard.min.css') }}">
{% endblock %}

{% block header %}

{% endblock %}

{% block container %}
    <div class="page-container">
        <!--/content-inner-->
        <div class="left-content">
            <div class="inner-content">

                <!-- header-starts -->
                <div class="header-section">

                    <!--menu-right-->
                    <div class="top_menu">

                        <!--/profile_details-->
                        <div class="profile_details_left">

                            <ul class="nofitications-dropdown">

                                <li class="dropdown note">
                                    <a href="{{ path('fos_message_inbox')}}"  title="notification des messages" ><i class="fa fa-envelope-o"></i> <span class="badge">3</span></a>
                                </li>

                                <li class="dropdown note">
                                    <a href="#" class="dropdown-toggle" title="notification des offres" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-bell-o"></i> <span class="badge">5</span></a>

                                    <ul class="dropdown-menu two">
                                        <li>
                                            <div class="notification_header">
                                                <h3>You have 5 new notification</h3>
                                            </div>
                                        </li>
                                        <li><a href="#">
                                                <div class="user_img"><img src="../assets/images/in.jpg" alt=""></div>
                                                <div class="notification_desc">
                                                    <p>Lorem ipsum dolor sit amet</p>
                                                    <p><span>1 hour ago</span></p>
                                                </div>
                                                <div class="clearfix"></div>
                                            </a></li>
                                        <li class="odd"><a href="#">
                                                <div class="user_img"><img src="../assets/images/in5.jpg" alt=""></div>
                                                <div class="notification_desc">
                                                    <p>Lorem ipsum dolor sit amet </p>
                                                    <p><span>1 hour ago</span></p>
                                                </div>
                                                <div class="clearfix"></div>
                                            </a></li>
                                        <li><a href="#">
                                                <div class="user_img"><img src="../assets/images/in8.jpg" alt=""></div>
                                                <div class="notification_desc">
                                                    <p>Lorem ipsum dolor sit amet </p>
                                                    <p><span>1 hour ago</span></p>
                                                </div>
                                                <div class="clearfix"></div>
                                            </a></li>
                                        <li>
                                            <div class="notification_bottom">
                                                <a href="#">See all notification</a>
                                            </div>
                                        </li>
                                    </ul>
                                </li>
                                <li class="dropdown note">
                                    <a href="#" class="dropdown-toggle" title="notification des demandes" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-copy"></i> <span class="badge blue1">9</span></a>
                                    <ul class="dropdown-menu two">
                                        <li>
                                            <div class="notification_header">
                                                <h3>You have 9 pending task</h3>
                                            </div>
                                        </li>
                                        <li><a href="#">
                                                <div class="task-info">
                                                    <span class="task-desc">Database update</span><span class="percentage">40%</span>
                                                    <div class="clearfix"></div>
                                                </div>
                                                <div class="progress progress-striped active">
                                                    <div class="bar yellow" style="width:40%;"></div>
                                                </div>
                                            </a></li>
                                        <li><a href="#">
                                                <div class="task-info">
                                                    <span class="task-desc">Dashboard done</span><span class="percentage">90%</span>
                                                    <div class="clearfix"></div>
                                                </div>

                                                <div class="progress progress-striped active">
                                                    <div class="bar green" style="width:90%;"></div>
                                                </div>
                                            </a></li>
                                        <li><a href="#">
                                                <div class="task-info">
                                                    <span class="task-desc">Mobile App</span><span class="percentage">33%</span>
                                                    <div class="clearfix"></div>
                                                </div>
                                                <div class="progress progress-striped active">
                                                    <div class="bar red" style="width: 33%;"></div>
                                                </div>
                                            </a></li>
                                        <li><a href="#">
                                                <div class="task-info">
                                                    <span class="task-desc">Issues fixed</span><span class="percentage">80%</span>
                                                    <div class="clearfix"></div>
                                                </div>
                                                <div class="progress progress-striped active">
                                                    <div class="bar  blue" style="width: 80%;"></div>
                                                </div>
                                            </a></li>
                                        <li>
                                            <div class="notification_bottom">
                                                <a href="#">See all pending task</a>
                                            </div>
                                        </li>
                                    </ul>
                                </li>
                                <div class="clearfix"></div>
                            </ul>
                        </div>
                        <div class="clearfix"></div>
                        <!--//profile_details-->
                    </div>
                    <!--//menu-right-->
                    <div class="clearfix"></div>
                </div>
                <!-- //header-ends -->

                <div class="outter-wp" style="margin-top: -6%;">
                    <div class="note" style="text-align: center;">
                        <a href="{{ path('market_index')}}" style="text-align: center;color: white;font-size: 16px;margin-bottom: 0%;background: #060;" class="badge">
                            <i class="fa fa-home"></i>  Le March√©</a>
                            {% block  haut %}   {% endblock  haut %}
                    </div>

                    <!--custom-widgets-->
                    <div class="custom-widgets">
                        <div class="row-one">

                        </div>
                    </div>
                    <!--//custom-widgets-->
                    <div class="candile">
                        <h3 class="sub-tittle"> </h3>
                        <div id="center">
                            <div id="fig" style="min-height: 280px;margin-left: 5%;">
                                <div class="custom-widgets">
                                    <div class="row-one">
                                        <p style="font-style: italic;font-size: 14px;text-align: left;"> <a href="{{ url('fos_message_inbox') }}">{% trans from 'FOSMessageBundle' %}inbox{% endtrans %}</a> <a href="{{ url('fos_message_sent') }}">{% trans from 'FOSMessageBundle' %}sent{% endtrans %}</a> <a href="{{ url('fos_message_deleted') }}">{% trans from 'FOSMessageBundle' %}deleted{% endtrans %}</a></p>
                                    {% block fos_message_content %}{% endblock %}
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <!--//outer-wp-->
            </div>
        </div>
    </div>
    <!--//content-inner-->
    <!--/sidebar-menu-->
    <div class="sidebar-menu">
        <header class="logo">
            <a href="#" class="sidebar-icon"> <span class="fa fa-bars"></span> </a> <a href="{{ path('dashboard_index')}}"> <span id="logo"> <h1>Benin Agro</h1></span>
                <!--<img id="logo" src="" alt="Logo"/>-->
            </a>
        </header>
        <div style="border-top:1px solid rgba(69, 74, 84, 0.7)"></div>
        <!--/down-->
        <div class="down">
            <a href="index.html"><img src="{{ asset('assets/images/admin.jpg')}}"></a>
            <a href="index.html"><span class=" name-caret">{{ app.user.lastName }} {{ app.user.firstName }}</span></a>
            <p> {{ app.user.profil }}</p>
            <ul>
                <li><a class="tooltips" href="index.html"><span>Profile</span><i class="fa fa-user-md"></i></a></li>
                <li><a class="tooltips" href="index.html"><span>Settings</span><i class="fa fa-wrench"></i></a></li>
                <li><a class="tooltips" href="{{ path('fos_user_security_logout') }}"><span>Log out</span><i class="fa fa-power-off"></i></a></li>
            </ul>
        </div>
        <!--//down-->
        <div class="menu">
            <ul id="menu" >

                <li>
                    <a href="{{ path('dashboard_mesoffres') }}" title="Mes Offres"><i class="fa fa-building-o"></i> <span>Mes Offres</span><span class="fa fa-angle-double-right" style="float: right"></span></a>
                </li>
                <li id="menu-academico" >
                    <a href="{{ path('dashboard_mesdemandes') }}" title="Mes demandes"><i class="fa fa-copy"></i> <span>Mes demandes</span> <span class="fa fa-angle-double-right" style="float: right"></span></a>
                </li>
                <li>
                    <a href="#" title="Corbeille"><i class="fa fa-trash-o"></i> <span>Corbeille</span> <span class="fa fa-angle-double-right" style="float: right"></span></a>
                </li>
                <li id="menu-comunicacao" >
                    <a href="#"  title="Consultations"><i class="fa fa-search"></i> <span>Consultations</span><span class="fa fa-angle-double-right" style="float: right"></span></a>
                </li>
            </ul>
        </div>
    </div>
    <div class="clearfix"></div>
</div>

{% endblock %}
{% block footer %} {% endblock %}
{% block javascripts %}
    {{ parent() }}
    <script>

        require(['dashboard/scripts']);
        require(['dashboard/index']);

    </script>
{% endblock %}